            $('#change_consent').on('click', function () {
                $('#cookie_consent').css('display', 'flex');
            });

            $('#cookie_consent .btn-allow').on('click', function () {
                setCookie('cookieconsent_status', 'allow', 365);
                $('#cookie_consent').css('display', 'none');
            });

            $('#cookie_consent .btn-deny').on('click', function () {
                setCookie('cookieconsent_status', 'deny', 365);
                window.location.href = "https://alphacoders.com/site/no-consent";
